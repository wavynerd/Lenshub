{
  "name": "lisfinity",
  "version": "1.0.0",
  "description": "Lisfinity - Classifieds Ads WordPress Theme",
  "main": "resources/js/main.js",
  "scripts": {
    "internal:pipeline": "webpack --colors",
    "lint": "run-s -cl lint:wpcs lint:scripts lint:styles",
    "lint-fix": "run-s -cl lint-fix:wpcs lint-fix:scripts lint-fix:styles",
    "lint:wpcs": "./wpcs/vendor/bin/phpcs --colors --standard=WordPress --ignore=includes/src/*,includes/woocommerce-product includes/",
    "lint:scripts": "eslint --color resources/statics/**",
    "lint:styles": "stylelint --color ./resources/styles/**",
    "lint-fix:wpcs": "./wpcs/vendor/bin/phpcbf --colors --standard=WordPress includes/",
    "lint-fix:scripts": "eslint --color --fix resources/statics/**",
    "lint-fix:styles": "stylelint --color --fix ./resources/styles/**",
    "i18n": "run-s -cl i18n:textdomain i18n:pot",
    "i18n:textdomain": "wpi18n addtextdomain --exclude=docs,node_modules,vendor,wpcs --textdomain=lisfinity",
    "i18n:pot": "wp i18n make-pot . languages/lisfinity.pot --exclude=docs,node_modules,vendor,wpcs,resources,dist",
    "theme": "node archiver.js",
    "start": "cross-env NODE_ENV=development SERVER=true webpack --watch",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "dev": "cross-env NODE_ENV=development webpack",
    "prod": "cross-env NODE_ENV=production webpack",
    "build": "concurrently \"npm run prod\" \"npm run dev\""
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^8.4.1",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.0.0",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-zip": "^5.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "import-glob": "^1.5.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.14.1",
    "node-wp-i18n": "^1.2.3",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.4",
    "resolve-url-loader": "^2.3.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.21.0",
    "tailwindcss": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "postcss-custom-properties": "^8.0.11",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "readline-sync": "^1.4.10",
    "regenerator": "^0.14.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "engines": {
    "node": ">=10.5.0",
    "npm": ">=6.4.1"
  },
  "author": "",
  "license": "ISC"
}
